{
    "name": "genomes/php-eth-tools",
    "type": "project",
    "description": "PHP Ethereum Tools, Message Signing, EC Recover, Key Generation.",
    "keywords": [
        "php",
        "ethereum",
        "ec_recover",
        "key generation"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.0",
        "digitaldonkey/ecverify": "^1.0",
        "kornrunner/keccak": "^1.0",
        "web3p/web3.php": "dev-master",
        "simplito/elliptic-php": "^1.0",
        "sop/asn1": "^4.0",
        "sop/crypto-encoding": "^0.3.0",
        "sop/crypto-types": "^0.3.0",
        "symfony/dotenv": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "EthereumTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test/"
        }
    }
}
